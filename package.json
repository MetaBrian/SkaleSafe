    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1"
